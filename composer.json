{
  "name": "northarea/php-amqp",
  "description": "Библиотека для работы с rabbitMQ с возможностью подключаться без расширения bcmath через http",
  "type": "library",
  "authors": [
    {
      "name": "Козлов Александр",
      "email": "adsl41c@yandex.ru",
      "role": "Developer"
    }
  ],
  "scripts": {
    "test": "phpunit --bootstrap vendor/autoload.php test"
  },
  "autoload": {
    "psr-4": {
      "Northarea\\PhpAmqp\\": "src/"
    },
    "classmap": [
      "src"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "test/"
    }
  },
  "license": ["MIT"],
  "require": {
    "php-amqplib/php-amqplib": "^2.9",
    "php": ">=7.1",
    "ext-json": "*",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4"
  }
}
